<html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js"></script>
<script type="text/javascript">


$(document).ready(function(){
	$("input[type='button']").click(function(){
		
		$.ajax({
			type: "GET",
		    url: "/video/end?id=222&index=132&length=22&user_id=2&active:true&token=a7556a56429952c671bb915023d7fb24bf84aa66&debug=true",
		    contentType: false,
		    processData: false
		});
	});


	var f = true;
	var handleFileSelect = function(evt) {
	    var files = evt.target.files;
	    var file = files[0];

	    var formData = new FormData();
	    formData.append('section', 'general');
		formData.append('action', 'previewImg');
		formData.append('video', file);
		formData.append('token', 'a7556a56429952c671bb915023d7fb24bf84aa66')
		formData.append('id',1);
		formData.append('index',132);
		formData.append('length',22);
		formData.append('user_id',2);
		formData.append('active',true);
		formData.append('description',"Description");
		formData.append('file_type','mp4');

		$.ajax({
			type: "POST",
		    url: "/video/upload?debug=true",
		    data: formData,
		    contentType: false,
		    processData: false
		});
	};

	if (window.File && window.FileReader && window.FileList && window.Blob) {
	    document.getElementById('filePicker').addEventListener('change', handleFileSelect, false);
	} else {
	    alert('The File APIs are not fully supported in this browser.');
	}
});


</script>
<body>
	<div>
    <div>
        <label for="filePicker">Choose or drag a file:</label><br>
        <input type="file" id="filePicker">
    </div>
    <div>
        <input type="button" value="end"></input>
    </div>
    <br>
    <div>
        <h1>Base64 encoded version</h1>
        <textarea id="base64textarea" placeholder="Base64 will appear here" cols="50" rows="15"></textarea>
    </div>
</div>
</body>
</html>